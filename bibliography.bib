%
% Reference: https://www.bibtex.com/e/article-entry/
% Reference Date: February 8, 2026
%
% Required:
% 1. author
% 2. title
% 3. journal
% 4. year
%
% Optional:
% 1. volume
% 2. number
% 3. pages
% 4. month
% 5. note
%
% Non-Standard:
% 1. doi
% 2. issn
% 3. zblnumber
% 4. eprint
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@article {%CITATION_KEY,
    author  = %STRING,
    title   = %STRING,
    journal = %STRING,
    year    = %INTEGER,
    volume  = %STRING,
    number  = %STRING,
    pages   = %STRING$
}

@book {%CITATION_KEY,
    author    = %STRING,
    title     = %STRING,
    publisher = %STRING,
    address   = %STRING,
    year      = %INTEGER
}

@mastersthesis {%CITATION_KEY,
    author  = %STRING,
    title   = %STRING,
    school  = %STRING,
    year    = %INTEGER,
    address = %STRING,
    month   = %ENUMERATION_KEY
}

@phdthesis {%CITATION_KEY,
    author = %STRING,
    title  = %STRING,
    school = %STRING,
    address = %STRING,
    year    = %INTEGER,
    month   = %ENUMERATION_KEY
}
